<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.OnlineProductApp" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryDark">@color/purple_700</item>
        <item name="colorAccent">@color/teal_200</item>
        <item name="android:windowActivityTransitions" tools:ignore="NewApi">true</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="cat_textview">
        <item name="android:button">@null</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@drawable/cat_textview_selector</item>
    </style>
</resources>